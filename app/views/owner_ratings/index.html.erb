<p id="notice"><%= notice %></p>

<h1>Owner Ratings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @owner_ratings.each do |owner_rating| %>
      <tr>
        <td><%= link_to 'Show', owner_rating %></td>
        <td><%= link_to 'Edit', edit_owner_rating_path(owner_rating) %></td>
        <td><%= link_to 'Destroy', owner_rating, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Owner Rating', new_owner_rating_path %>
